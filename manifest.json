{
  "manifest_version": 2,

  "name": "Click to execute",
  "description": "lalalalal",
  "version": "1.0",

  "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [ "jquery.min.js" ]
        }
    ],

  "permissions": [
    "tabs", "<all_urls>"
  ],

  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "script.js"
  ]
}